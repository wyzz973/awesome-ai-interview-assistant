{
  "name": "ai-interview-assistant",
  "version": "0.1.0",
  "description": "AI 面试助手 - 基于 Electron 的智能面试辅助工具",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "package": "npm run build && electron-builder --mac --config electron-builder.yml",
    "package:dmg": "npm run build && electron-builder --mac dmg --config electron-builder.yml",
    "package:zip": "npm run build && electron-builder --mac zip --config electron-builder.yml",
    "package:dir": "npm run build && electron-builder --mac dir --config electron-builder.yml"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.2.0",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.24",
    "better-sqlite3": "^12.6.2",
    "electron-log": "^5.4.3",
    "electron-store": "^11.0.2",
    "jspdf": "^4.1.0",
    "keytar": "^7.9.0",
    "lucide-react": "^0.574.0",
    "markdown-it": "^14.1.1",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.2.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/markdown-it": "^14.1.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^33.2.1",
    "electron-builder": "^26.8.1",
    "electron-vite": "^2.3.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vitest": "^4.0.18"
  }
}
